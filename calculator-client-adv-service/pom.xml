        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>com.library</groupId>
    <artifactId>calculator-client-adv-service</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>calculator-client-adv-service</name>
    <description>calculator-client-adv-service</description>
    <url/>
    <licenses>
        <license/>
    </licenses>
    <developers>
        <developer/>
